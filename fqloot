ItemDisplay[cm1 ID RES>1]: %GREEN%%RES% All Resist %CONTINUE%									// +All Resist
ItemDisplay[cm1 ID DEF>19]: %GREEN% %DEF% Defence %CONTINUE%									// +19 Defence
ItemDisplay[cm1 ID LIFE>17]: %GREEN% +%STAT(7,0)% Life %CONTINUE%								// +18 Life
ItemDisplay[cm1 ID STAT22=3]: %PURPLE%+3 Dmg %CONTINUE%									// +3 Damage
ItemDisplay[cm1 ID STAT22=2]: %ORANGE%+2 Dmg %CONTINUE%									// +2 Damage
ItemDisplay[cm1 ID STAT22=1]: %WHITE%+1 Dmg %CONTINUE%									// +1 damage
ItemDisplay[cm1 ID (STAT57>0 AND STAT58<210) STAT58>150]: %GREEN% %STAT(57,0)%%STAT(58,0)% PoisonDmg %CONTINUE%	// +100 Poison
ItemDisplay[cm1 ID (STAT57>0 AND STAT58>220)]: %GREEN% %STAT(57,0)%-%STAT(58,0)% PoisonDmg %CONTINUE%			// +200 Poison
ItemDisplay[cm1 ID (STAT48>0 AND STAT49>9)]: %RED%  %STAT(48,0)%-%STAT(49,0)% FireDmg %CONTINUE%			// +10 Fire Damage
ItemDisplay[cm1 ID (STAT54>0 AND STAT55>9)]: %BLUE%  %STAT(54,0)%-%STAT(55,0)% ColdDmg %CONTINUE% 			// +10 Cold Damage
ItemDisplay[cm1 ID (STAT50>0 AND STAT51>29)]: %YELLOW%  %STAT(51,0)% LightDmg %CONTINUE%				// +30 Lightning Damage
// -----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
//	Small Charms		Elemental Resistances (39,41,43,45)																				|
// -----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
//				Life		Attack		Damage		PoisonDmg	FireDmg		ColdDmg		LightDmg	
ItemDisplay[cm1 ID ((DEF>0 OR LIFE>0 OR STAT19>0 OR STAT22>0 OR STAT57>0 OR STAT48>0 OR STAT54>0 OR STAT50>0) AND STAT39>0)]: %NAME%%RED% +%STAT(39,0)% FireRes %CONTINUE%		// Fire Resist
ItemDisplay[cm1 ID ((DEF>0 OR LIFE>0 OR STAT19>0 OR STAT22>0 OR STAT57>0 OR STAT48>0 OR STAT54>0 OR STAT50>0) AND STAT41>0)]: %NAME%%YELLOW% +%STAT(41,0)% LightRes %CONTINUE%	// Light Resist
ItemDisplay[cm1 ID ((DEF>0 OR LIFE>0 OR STAT19>0 OR STAT22>0 OR STAT57>0 OR STAT48>0 OR STAT54>0 OR STAT50>0) AND STAT43>0)]: %NAME%%BLUE% +%STAT(43,0)% ColdRes %CONTINUE%		// Cold Resist 
ItemDisplay[cm1 ID ((DEF>0 OR LIFE>0 OR STAT19>0 OR STAT22>0 OR STAT57>0 OR STAT48>0 OR STAT54>0 OR STAT50>0) AND STAT45>0)]: %NAME%%GREEN% +%STAT(45,0)% PoisonRes %CONTINUE%	// Poison Resist
//
ItemDisplay[cm1 ID RES>1]: %GREEN%%RES% All Resist %CONTINUE%						// +All Resist
